<?xml version="1.0"?>
<extension key="com.ginkgostreet.cividiscount.rbp" type="module">
  <file>rbp</file>
  <name>CiviDiscount Redeem by Participant</name>
  <description>
    Modifies CiviDiscount to optionally change the basis of the usage count for
    a discount program from transactions to participants.
  </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Ginkgo Street Labs</author>
    <email>inquire@ginkgostreet.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ginkgostreet/com.ginkgostreet.cividiscount.rbp</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-08-21</releaseDate>
  <version>0.1</version>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Rbp</namespace>
  </civix>
</extension>
